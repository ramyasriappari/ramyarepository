package com.collection;
import java.util.HashSet;

public class Collections {

	public static void main(String[] args) {
		HashSet<Integer> set =new HashSet<Integer>();
		set.add(50);
		set.add(10);
		set.add(20);
		set.add(30);
		set.add(40);
		set.add(60);
		System.out.println(set);
		HashSet<Integer> ob =new HashSet<Integer>();
		ob.add(23);
		ob.add(33);
		System.out.println(ob);//to add elements to the existing class
	    set.clear();//clear all elements
	    System.out.println("To clear all elements "+set);
		HashSet<String> ob1 =new HashSet<String>();
		ob1.add("Ramya");
		ob1.add("Sowji");
		ob1.add("Sarada");
		System.out.println(set);
		System.out.println(ob1.contains("Ramya"));//it contains String or not
		ob1.remove("Sowji");//this method is used to remove particular element
		

	}

}
