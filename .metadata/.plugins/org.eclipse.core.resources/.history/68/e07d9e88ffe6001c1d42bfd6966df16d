package com.collection;
import java.util.TreeSet;

public class Collections {

	public static void main(String[] args) {
		TreeSet<Integer> set =new TreeSet<Integer>();
		set.add(50);
		set.add(10);
		set.add(20);
		set.add(30);
		set.add(40);
		set.add(60);
		System.out.println(set);
		TreeSet<Integer> ob =new TreeSet<Integer>();
		ob.add(23);
		ob.add(33);
		System.out.println(ob);//to add elements to the existing class
	    set.clear();//clear all elements
	    System.out.println("To clear all elements "+set);
		TreeSet<String> ob1 =new TreeSet<String>();
		ob1.add("Ramya");
		ob1.add("Sowji");
		ob1.add("Sarada");
		System.out.println(set);
		System.out.println(ob1.contains("Ramya"));//it contains String or not
		ob1.remove("Sowji");//this method is used to remove particular element
		System.out.println(ob1);
		System.out.println(ob1.isEmpty());//this method is used to find set is empty or not
		System.out.println(ob1.size());//this method is used to find the set of size
		System.out.println(ob1.equals(set));//this method is used to find values in set and ob1 are same or not
		ob1.removeAll(ob1);
		
	}

}
