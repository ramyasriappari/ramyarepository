package com.train;
abstract interface Train
{
	public void travelling();
	public void travel()
	{
		System.out.println("Reached ");
	}
}
 class Narsapur extends Train
 {
	 void travelling()
	 {
		 System.out.println("from station is Nsp");
		 System.out.println("to station is Hyd");
	 }
	 
 }
 class VishakhaExp extends Train
 {
	 void travelling()
	 {
		 System.out.println("from station is Vizag");
		 System.out.println("to station is Araku");
	 }
	 
 }
public class DemoTrain {

	public static void main(String[] args) {
		Train  ob=new Narsapur();
	    ob.travelling();
	    ob.travel();
	    
	    ob=new VishakhaExp();
	    ob.travelling();
	    ob.travel();
	    

	}

}
