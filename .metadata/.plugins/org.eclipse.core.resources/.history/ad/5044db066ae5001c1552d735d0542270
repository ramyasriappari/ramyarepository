package com.train;
abstract class Train2
{
	abstract void travelling();
	 void travel()
	 {
		 System.out.println("Reached safely");
	 }
	
}
class Narsapur2 extends Train1
{
	 void travelling()
	 {
		 String name;
		 System.out.println("from station is Nsp");
		 System.out.println("to station is Hyd");
		 super.name="ramya";
		 
	 }
	 
}
class VishakhaExp2 extends Narsapur2
{
	 void travelling()
	 {
		 System.out.println("from station is Vizag");
		 System.out.println("to station is Araku");
	 }
	 
}


