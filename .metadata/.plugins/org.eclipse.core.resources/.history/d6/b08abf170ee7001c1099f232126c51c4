package com.exception;

public class ArrayStoreException {

	public static void main(String[] args) {
	String [] Array=new String[6];
	Array[0]="ramya";
	System.out.println(Array[2]);
	try
	{
		Array[0]="12";
		System.out.println(Array[0]);
	}
	catch(Exception g)
	{
		System.out.println(g);
	}
	
		
	}

}
